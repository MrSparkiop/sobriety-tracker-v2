{
  "name": "sobriety-tracker-v2",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://MrSparkiop.github.io/sobriety-tracker-v2",
  "dependencies": {
    "@sentry/cli": "^2.46.0",
    "@sentry/node": "^9.24.0",
    "@sentry/react": "^9.24.0",
    "@sentry/tracing": "^7.120.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.5.0",
    "axios": "^1.7.1",
    "lucide-react": "^0.395.0",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.1",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1"
  },
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build && npm run sentry:sourcemaps",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org blagoy --project javascript-react ./out && sentry-cli sourcemaps upload --org blagoy --project javascript-react ./out"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
